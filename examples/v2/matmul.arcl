incl math.Mat;

main <> {
    decl Mat:flt 3 3: A = Mat:flt 3 3:from ((1.,0.,1.), 
                                            (0.,1.,0.), 
                                            (1.,0.,1.));

    decl Mat:flt 3 3: B = Mat:flt 3 3:from ((1.,2.,3.), 
                                            (4.,5.,6.), 
                                            (7.,8.,9.));

    decl Mat:flt 3 3: C = A*B;
};