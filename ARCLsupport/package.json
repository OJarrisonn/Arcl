{
  "name": "arcl",
  "displayName": "ARCL",
  "description": "ARCL language support",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.83.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "arcl",
      "aliases": ["ARCL", "arcl"],
      "extensions": [".arcl"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "arcl",
      "scopeName": "source.arcl",
      "path": "./syntaxes/arcl.tmLanguage.json"
    }]
  }
}
